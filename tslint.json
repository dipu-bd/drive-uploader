{
  "extends": [
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "no-unused-variable": false,
    "indent": [true, "spaces", 2],
    "trailing-comma": {
      "options": {
        "multiline": "always",
        "singleline": "never"
      }
    },
    "space-before-function-paren": {
      "options": {
        "asyncArrow": "always",
        "anonymous": "always",
        "named": "never"
      }
    }
  },
  "rulesDirectory": []
}
